{
	"compilerOptions": {
		"outDir": "dist",
		"target": "es2019",
		"lib": ["es2019"],
		"allowJs": true,
		"module": "commonjs",
		"esModuleInterop": true,
		"strict": true,
		"declaration": true,
		"sourceMap": true
	},
	"include": ["src/**/*", "./"],
	"exclude": ["node_modules", "__tests__", "public", "tmp"]
}
