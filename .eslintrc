{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "strict": ["error", "never"],
    "no-mixed-spaces-and-tabs": "off",
    "object-curly-spacing": ["error", "always"]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
